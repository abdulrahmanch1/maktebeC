{
  "version": 2,
  "builds": [
    {
      "src": "maktebe/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "npm run build"
      }
    },
    {
      "src": "maktebe/api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "maktebe/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "maktebe/build/index.html"
    }
  ]
}