{
  "version": 2,
  "builds": [
    {
      "src": "maktebe/backend/server.js",
      "use": "@vercel/node",
      "name": "backend-api"
    },
    {
      "src": "maktebe/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "maktebe/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "maktebe/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/maktebe/build/index.html"
    }
  ]
}